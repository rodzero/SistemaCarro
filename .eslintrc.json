{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "globals": {
        "AppCarro": true,
        "TPC": true,
        "Carro": true,
        "PNotify": true,
        "$": true,
        "idSimulacao": true,
        "Geocoder": true,
        "google": true,
        "Simulacao": true
    }
}
